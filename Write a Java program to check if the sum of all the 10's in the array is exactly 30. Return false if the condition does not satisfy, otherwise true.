import java.util.*;

public class Exercise {
    public static void main(String[] args) throws FileNotFoundException {
        int[] array_nums = {10, 77, 10, 54, -11, 10};
        System.out.println(sum(array_nums, 10, 30));


    }

    public static boolean sum(int[] arr, int search_num, int fixed_sum){
        int sum = 0;
        for(int i = 0; i < arr.length; i++){
            if(arr[i] == search_num){
                sum+= arr[i];
            }
        }

        if(sum == fixed_sum){
            return true;
        } else {
            return false;
        }
    }


}
